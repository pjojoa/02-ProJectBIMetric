{
    "dataRoles": [
        {
            "displayName": "Urn",
            "name": "urn",
            "kind": "Grouping"
        },
        {
            "displayName": "ExternalIds",
            "name": "externalIds",
            "kind": "Grouping"
        },
        {
            "displayName": "Color",
            "name": "color",
            "kind": "Grouping"
        }
    ],
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "urn"
                            }
                        },
                        {
                            "for": {
                                "in": "externalIds"
                            }
                        },
                        {
                            "for": {
                                "in": "color"
                            }
                        }
                    ],
                    "dataReductionAlgorithm": {
                        "window": {
                            "count": 30000
                        }
                    }
                }
            }
        }
    ],
    "objects": {
        "viewer": {
            "properties": {
                "accessTokenEndpoint": {
                    "type": {
                        "text": true
                    }
                }
            }
        },
        "dataPoint": {
            "displayName": "Data Colors",
            "properties": {
                "fill": {
                    "displayName": "Fill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showColor": {
                    "displayName": "Show Color",
                    "type": {
                        "bool": true
                    }
                }
            }
        }
    },
    "privileges": [
        {
            "name": "WebAccess",
            "essential": true,
            "parameters": [
                "*"
            ]
        }
    ],
    "supportsMultiVisualSelection": true,
    "supportsHighlight": false,
    "suppressDefaultTitle": true,
    "supportsLandingPage": true,
    "supportsEmptyDataView": true
}